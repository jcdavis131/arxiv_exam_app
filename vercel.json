{
  "version": 2,
  "builds": [

    {
      "src": "arxiv_exam_app.py",
      "use": "@vercel/python",
      "config": {
        "excludeFiles": "{**/*.py[cod],**/__pycache__/**,tests/**,scripts/**,examples/**,docs/**}"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "arxiv_exam_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "LOG_LEVEL": "INFO"
  }
}